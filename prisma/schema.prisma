generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Comp {
  id                              Int     @id @default(autoincrement())
  tanggal                         DateTime
  hot_side_first_temp             Decimal
  cold_side_first_temp            Decimal
  hot_side_second_temp            Decimal
  cold_side_second_temp           Decimal
  hot_side_third_temp             Decimal
  cold_side_third_temp            Decimal
  oil_pressure                    Decimal
  water_inlet_pressure            Decimal
  water_outlet_temp               Decimal
  second_stage_cylinder_water_temp Decimal
  third_stage_cylinder_water_temp Decimal
}
